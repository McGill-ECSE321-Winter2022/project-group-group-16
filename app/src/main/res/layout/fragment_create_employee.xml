<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/error"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginBottom="731dp"
        android:text=""
        android:textColor="#FF0000"
        android:translationX="100dp"
        android:translationY="0dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="274dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="36dp"
        android:onClick="createEmployee"
        android:text="Save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="307dp"
        android:layout_height="53dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toTopOf="@+id/editTextDate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usernameId" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="149dp"
        android:layout_height="53dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="26dp"
        android:ems="10"
        android:hint="Date Hired"
        android:inputType="date"
        android:minHeight="48dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputEditText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editTextDate2"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="135dp"
        android:layout_height="48dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="26dp"
        android:ems="10"
        android:hint="Date of Birth"
        android:inputType="date"
        android:minHeight="48dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberDecimal"
        app:layout_constraintEnd_toStartOf="@+id/editTextDate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" />

    <TextView
        android:id="@+id/createEmployee"
        android:layout_width="256dp"
        android:layout_height="41dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="29dp"
        android:text="Create Employee"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/streetNameId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="138dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="21dp"
        android:ems="10"
        android:hint="Hourly Pay"
        android:inputType="numberDecimal"
        android:minHeight="48dp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toStartOf="@+id/textInputEditText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDate2" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="36dp"
        android:onClick="managerHomePage"
        android:text="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText"
        android:layout_width="152dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="74dp"
        android:layout_marginBottom="21dp"
        android:hint="Employee Status"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editTextNumberDecimal"
        app:layout_constraintTop_toBottomOf="@+id/editTextDate" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/firstNameId"
        android:layout_width="138dp"
        android:layout_height="56dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="21dp"
        android:hint="First Name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/usernameId"
        app:layout_constraintEnd_toStartOf="@+id/passwordId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/countryId" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/usernameId"
        android:layout_width="138dp"
        android:layout_height="56dp"
        android:layout_marginStart="27dp"
        android:layout_marginBottom="25dp"
        android:hint="Username"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress"
        app:layout_constraintEnd_toStartOf="@+id/passwordId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstNameId" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/passwordId"
        android:layout_width="138dp"
        android:layout_height="56dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="25dp"
        android:hint="Password"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/usernameId"
        app:layout_constraintTop_toBottomOf="@+id/LastNameId"
        app:layout_constraintVertical_bias="0.487" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/LastNameId"
        android:layout_width="138dp"
        android:layout_height="56dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="21dp"
        android:hint="Last Name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/passwordId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/firstNameId"
        app:layout_constraintTop_toBottomOf="@+id/postalCodeId" />

    <EditText
        android:id="@+id/countryId"
        android:layout_width="141dp"
        android:layout_height="48dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="24dp"
        android:layout_weight="1"
        android:ems="10"
        android:hint="@string/country"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/firstNameId"
        app:layout_constraintEnd_toStartOf="@+id/postalCodeId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cityId" />

    <EditText
        android:id="@+id/postalCodeId"
        android:layout_width="170dp"
        android:layout_height="48dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="24dp"
        android:layout_weight="1"
        android:ems="10"
        android:hint="@string/postal_code"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/LastNameId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/countryId"
        app:layout_constraintTop_toBottomOf="@+id/provinceId" />

    <EditText
        android:id="@+id/cityId"
        android:layout_width="149dp"
        android:layout_height="48dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="16dp"
        android:layout_weight="1"
        android:ems="10"
        android:hint="@string/city"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/countryId"
        app:layout_constraintEnd_toStartOf="@+id/provinceId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/streetNumberId" />

    <EditText
        android:id="@+id/provinceId"
        android:layout_width="152dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="16dp"
        android:layout_weight="1"
        android:ems="10"
        android:hint="@string/province"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/postalCodeId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cityId"
        app:layout_constraintTop_toBottomOf="@+id/streetNameId" />

    <EditText
        android:id="@+id/streetNameId"
        android:layout_width="183dp"
        android:layout_height="48dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        android:ems="10"
        android:hint="@string/street_name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/provinceId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/streetNumberId"
        app:layout_constraintTop_toBottomOf="@+id/createEmployee" />

    <EditText
        android:id="@+id/streetNumberId"
        android:layout_width="128dp"
        android:layout_height="48dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        android:ems="10"
        android:hint="@string/street_number"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/cityId"
        app:layout_constraintEnd_toStartOf="@+id/streetNameId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/createEmployee" />

</androidx.constraintlayout.widget.ConstraintLayout>